  },
  "homepage": "https://github.com/scrythe/underfight#readme",
  "devDependencies": {
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.16",
    "standard-version": "^9.5.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
